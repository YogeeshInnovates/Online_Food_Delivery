<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Food Items</title>
</head>
<body>
    <%-include('partials/nav.ejs')%>
    <h1>Food Items</h1>
    <% items.forEach(item => { %>
        <div>
            <h2><%= item.item_name %></h2>
            <p>Quantity: <%= item.price %></p>
            <p>Price: <%= item.id %></p>
            <img src="<%= item.base64Image %>" alt="<%= item.Fname %> Image">
        </div>
    <% }); %>
    <a href="#" id="cart-icon" onclick="addItemToCart()">click</a>
        <script src="/js/cart.js"></script>
</body>
</html>
